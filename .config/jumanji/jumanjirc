searchengine g http://www.google.com/search?q=%s
searchengine k http://kickass.so/usearch/?q=%s
searchengine y https://www.youtube.com/results?search_query=%s

set homepage file:///home/veggie/scripts/home.html
set auto_save 60
set default_session_name start
set session_restored true

set proxy localhost:8118
set close-window-with-last-tab false

script ~/.config/jumanji/scripts/hints.js

set download_dir ~/Downloads/
set download_command urxvt -e sh -c "wget --load-cookies ~/.config/jumanji/cookies '%s' -O '%s'";
