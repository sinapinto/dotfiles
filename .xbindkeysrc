######### Apps
"urxvt"
    Mod1 + Return

"jumanji &>/dev/null"
    Mod1 + r

"chromium &>/dev/null"
    Mod1 + c

######## Music
"mpc toggle"
    Mod1 + p

"mpc seek +15"
    Pause

"mpc seek -15"
    Print

"mpc prev"
    Mod1 + i

"mpc next"
    Mod1 + o

###### Volume
"amixer set Master 3%+ unmute"
    F2

"amixer set Master 3%- unmute"
    F1

#"amixer set Master toggle"
    #F4

"amixer set Master 3%+ unmute"
    XF86AudioRaiseVolume

"amixer set Master 3%- unmute"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

####### Backlight
"xbacklight -inc 5"
   Mod1 + F10

"xbacklight -dec 5"
   Mod1 + F9

