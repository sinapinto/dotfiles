######### Apps
"termite"
    Mod1 + t

"chromium"
    Mod1 + c

"firefox"
    Mod1 + w

#"dmenu_run"
"rofi -show run"
    Mod1 + d

######## Music
"mpc toggle"
    Mod1 + p

"mpc seek +30"
    Pause

"mpc seek -30"
    Print

"mpc prev"
    Mod1 + i

"mpc next"
    Mod1 + o

###### Volume
"amixer set Master 3%+ unmute"
    F2

"amixer set Master 3%- unmute"
    F1

"amixer set Master toggle"
    F4

"amixer set Master 3%+ unmute"
    XF86AudioRaiseVolume

"amixer set Master 3%- unmute"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

