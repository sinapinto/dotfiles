######### Apps
"urxvt"
    Mod1 + Return

"pomf"
    Mod1 + bracketright

"urxvt -e vim $HOME/scripts/.todo"
    Menu

"jumanji &>/dev/null"
    Mod1 + r

"chromium &>/dev/null"
    Mod1 + c

"dwb &>/dev/null"
    Mod1 + e

######## Music
"mpc toggle"
    Mod1 + p

"mpc seek +5"
    Pause
"mpc seek -5"
    Print

"mpc prev"
    Mod1 + i

"mpc next"
    Mod1 + o

###### Volume
"amixer set Master 3%+ unmute"
    F2

"amixer set Master 3%- unmute"
    F1

#"amixer set Master toggle"
    #F4

"amixer set Master 3%+ unmute"
    XF86AudioRaiseVolume

"amixer set Master 3%- unmute"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

# Backlight
"xbacklight -inc 10"
   Mod1 + F10
"xbacklight -dec 10"
   Mod1 + F9

# vi:syntax=sh
