######### Apps
"urxvt"
    Mod1 + Return

"chromium &>/dev/null"
    Mod1 + c

"dmenu_run"
    Mod1 + d

######## Music
"mpc toggle"
    Mod1 + p

"mpc seek +30"
    Pause

"mpc seek -30"
    Print

"mpc prev"
    Mod1 + i

"mpc next"
    Mod1 + o

###### Volume
"amixer set Master 3%+ unmute"
    F2

"amixer set Master 3%- unmute"
    F1

"amixer set Master toggle"
    F4

"amixer set Master 3%+ unmute"
    XF86AudioRaiseVolume

"amixer set Master 3%- unmute"
    XF86AudioLowerVolume

"amixer set Master toggle"
    XF86AudioMute

# vi:syntax=sh
