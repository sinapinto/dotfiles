URxvt*termName:             rxvt-256color
URxvt*geometry:             80x20
URxvt.scrollBar:            false

URxvt*perl-ext-common:      default,matcher,url-select
URxvt*url-select.launcher:  /home/veggie/bin/launch
URxvt*matcher.button:       1
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next

URxvt*fading: 20

URxvt*colorUL:#96A8B7
URxvt*underlineColor: #96A8B7

!URxvt*font:-nil-profont-medium-r-normal--12-120-72-72-c-60-iso8859-1
!URxvt*boldFont:-nil-profont-medium-r-normal--12-120-72-72-c-60-iso8859-1

!URxvt*font:xft:Terminess Powerline:size=8
!URxvt*boldFont:xft:Terminess Powerline:size=8:style=bold

URxvt*font:xft:Anonymice Powerline:size=10:style=Bold
URxvt*boldFont:xft:Anonymice Powerline:size=10:style=Bold
URxvt*letterSpace:-1.0

Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
