URxvt.perl-ext-common: resize-font,matcher

URxvt.url-launcher: bin/launch
URxvt.matcher.button: 1
URxvt.keysym.M-C-u: perl:matcher:last
URxvt.keysym.M-u: perl:matcher:list
URxvt.matcher.rend.0: Uline Bold fg5

URxvt.keysym.C-S-Up:   resize-font:bigger
URxvt.keysym.C-S-Down: resize-font:smaller

URxvt*termName:       rxvt-256color
URxvt*internalBorder: 0
! disable ctrl+shift
URxvt*iso14755:       false
URxvt*scrollBar:      false
URxvt*colorUL:        #458588
URxvt*underlineColor: #458588

URxvt*boldFont:    xft:Liberation Mono:pixelsize=14:style=Bold,xft:Symbola
URxvt*font:        xft:Liberation Mono:pixelsize=14:style=Normal,xft:Symbola
URxvt*letterSpace: -1

*background: #282828
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

Xft.antialias: 1
Xft.autohint:  0
Xft.dpi:       96
Xft.hinting:   1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba:      rgb
! vim: ft=xdefaults cms=!%s
