URxvt*termName:             rxvt-256color
URxvt*geometry:             80x20
URxvt.scrollBar:            false
!
URxvt*perl-ext-common:      default,matcher,url-select
URxvt*url-select.launcher:  /home/veggie/bin/launch
URxvt*matcher.button:       1
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next

!URxvt*transparent: true
URxvt*fading: 25

*background: #191919
*foreground: #bcbcaf
!
! --- standard colors ---
!
! black
*color0:    #262524
!
! bright_black
*color8:    #46423b
!
! red
*color1:    #a04363
!
! bright_red
*color9:    #ae837a
!
! green
*color2:    #9b9329
!
! bright_green
*color10:   #b4aa30
!
! yellow
*color3:    #bf7a29
!
! bright_yellow
*color11:   #c7a551
!
! blue
*color4:    #6a8c8c
!
! bright_blue
*color12:   #74999e
!
! magenta
*color5:    #856774
!
! bright_magenta
*color13:   #9c818e
!
! cyan
*color6:    #757978
!
! bright_cyan
*color14:   #9fa590
!
! white
*color7:    #bcbcaf
!
! bright_white
*color15:   #bcbcaf

URxvt*colorUL:                 #96A8B7
URxvt*underlineColor:          #96A8B7

!URxvt*font:-nil-profont-medium-r-normal--12-120-72-72-c-60-iso8859-1
!URxvt*boldFont:-nil-profont-medium-r-normal--12-120-72-72-c-60-iso8859-1

URxvt*font:xft:Terminess Powerline:size=8
URxvt*boldFont:xft:Terminess Powerline:size=8:style=bold
!
!URxvt*font:xft:DejaVu Sans Mono for Powerline:size=8:style=Bold
!URxvt*boldFont:xft:DejaVu Sans Mono for Powerline:size=8:style=Bold
!URxvt*letterSpace:-1.9

Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
